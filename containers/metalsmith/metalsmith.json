{
  "source": "/monera/src/content/",
  "destination": "/monera/dist",
  "plugins": {
    "metalsmith-drafts": true,
    "metalsmith-markdown": true,
    "metalsmith-collections": {
      "pattern": "*.md",
      "sortBy": "date",
      "reverse": true
    },
    "metalsmith-permalinks": "posts/:title",
    "metalsmith-layouts": {
      "engine": "handlebars",
      "default": "default.html",
      "directory": "layouts/",
      "partials": "partials/"
    },
    "metalsmith-ignore": true
  }
}
